#include<stdio.h>

//FUN() Declaration
void printTable(int Table[2][10],int x, int y);

//main Fun()
int main()
{
    int m,n;
    printf("Enter the first number:");
    scanf("%d",&m);
    printf("Enter the second number:");
    scanf("%d",&n);
    int Table[2][10];
    //Function CALL
    printTable(Table,m,n);

}

//FUN() Definition
void printTable(int Table[2][10],int x, int y)
{
    for(int i=0;i<1;i++)
    {
        for(int j=0;j<=10;j++)
        {
            Table[i][j]=x*j;
            printf("%d X %d =%d\n",x,j,Table[i][j]);
        }
    }
    {
    for(int i=1;i<2;i++)
    {
        for(int j=0;j<=10;j++)
        {
            Table[i][j]=y*j;
            printf("%d X %d =%d\n",y,j,Table[i][j]);
        }
    }
}
}
