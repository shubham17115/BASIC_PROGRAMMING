#include<stdio.h>

int main()
{
    int L= 0;
    int n;
    printf("Enter the size of the array:");
    scanf("%d",&n);
    int largest[n];
    for(int i=0;i<n;i++)
    {
    printf("Enter the element at position [%d]:",i);
    scanf("%d",&largest[i]);
    }
    for(int i=0;i<n;i++)
    {
        if(L<largest[i])
        {
           L=largest[i]; 
        }
        else
        {
            L=L;
        }
        
    }
    printf(" The largest number in the array is:%d",L);

}
